 <%-include("header")-%>

<div class="box " id="heading">
  <h1> <%= ListTitle %> </h1>
</div>
<div class="box">

<%  itemLists.forEach(function(itemList){  %>
  <form action="/delete" method="post">
  <div class="item" >
    <input type="checkbox" name="checkbox" value="<%=itemList._id%>" onChange="this.form.submit()" >
    <p> <%= itemList.name %> </p>
  </div>
  <input type="hidden"  name="listName" value= "<%= ListTitle %>" >
  </form>
   <% });%>

        <form  action="/" method="post">
          <input type="text"class="list" name="newItem"  placeholder="New Item"  autocomplete="off">
          <button type="submit" name="list" value= "<%= ListTitle %>">+</button>

        </form>
</div>

 <%-include("footer")-%>
